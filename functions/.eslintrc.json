{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "indent": "off",
    "no-underscore-dangle": 0,
    "func-names": 0,
    "max-len": ["warn"],
    "no-console": ["warn"],
    "no-unused-vars": ["warn"],
    "prefer-arrow-callback": ["warn"],
    "default-param-last": 0,
    "object-curly-spacing": 0,
    "linebreak-style": ["error", "windows"]
  }
}
